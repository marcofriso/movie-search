{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "off",
    "no-nested-ternary": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "prefer-rest-params": "off",
    "func-names": "off",
    "jsx-a11y/label-has-associated-control": [ 2, {
      "required": {
          "some": [ "nesting", "id" ]
      }
    }]
  }
}